<h1 class="graph"><a href="/">Graphic Baseball</a> | <%= @title %></h1>
<p class="league">National League</p>
<p class="league" style="margin-left: 10.5%;">American League</p>
<p class="league" style="margin-left: 12%; background: #FFF">Toggle series by clicking on teams in legend.</p>
<p class="league" style="margin-left: 15.5%;">Select Year</p>

<form class="buttonform">
	<input type="button" id="nlwest" value="West" />
	<input type="button" id="nlcent" value="Central" />
	<input type="button" id="nleast" value="East" />
	<input type="button" id="nlwildcard" value="Wildcard"/>
	<input type="button" id="alwest" value="West" />
	<input type="button" id="alcent" value="Central" />
	<input type="button" id="aleast" value="East" />
	<input type="button" id="alwildcard" value="Wildcard"
	<input type="button" id="custom" value="Custom" />
	
</form>

<div id= "standings" style="width: 1200px; height: 460px;" ></div>

<script type="text/javascript">
	var standingselect=document.getElementById("standinglist")
		standingselect.onchange = function(){ 
	var selectedyear=this.options[this.selectedIndex] 
		window.open(selectedyear.value, "_self", "")} //"_self" redirects current window
	
</script>

